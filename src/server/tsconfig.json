{
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "../../dist/",
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "allowSyntheticDefaultImports": true,
    "lib": ["es2017", "es2019"],
    "types": ["node", "express"],
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true
  },
  "include": ["./**/*"],
  "exclude": ["../../node_modules"]
}
